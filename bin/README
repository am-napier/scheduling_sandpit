python3 gen_searches.py -c "*/2 * * * *" -p 3 -w 0 -n 50 win-0-highest -x highest

python3 gen_searches.py -c "*/2 * * * *" -p 3 -n 50 skew-100p-normal -s 100%
python3 gen_searches.py -c "*/2 * * * *" -p 3 -n 50 skew-50p-normal -s 50%
python3 gen_searches.py -c "*/2 * * * *" -p 3 -n 50 skew-1m-normal -s 1m

